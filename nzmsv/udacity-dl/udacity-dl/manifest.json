{
    "name": "Udacity Video Downloader",
    "version": "0.14.104",
    "manifest_version": 2,
    "description": "Download videos from Udacity for offline viewing.",
    "homepage_url": "http://www.nzmsv.com/udacity-dl/",
    "content_scripts" : [
        {
            "matches" : [
                "http://*.udacity.com/view*"
            ],
            "js" : ["contentscript.js"]
        }
    ],
    "permissions": [
        "http://*.udacity.com/*",
        "http://*.youtube.com/*"
    ],
    "web_accessible_resources": [
        "throbber.gif"
    ],
    "update_url": "http://www.nzmsv.com/udacity-dl/update.xml"
}
